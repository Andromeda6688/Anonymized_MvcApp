@model List<MvcApplication1.Models.PagesMenuItem>
@{
    Layout = "~/Views/Administration/_AdminLayout.cshtml";
}

<h1>Управление страницами</h1>

@foreach (var page in Model)
{
    <p>
        <a href="@Url.Action("Page", "Administration", new {pageId = page.Id})">
            @page.Title </a>
    </p>
}
<input class="approve_button" type="button" value="Новая страница"
       onclick="location.href='@Url.Action("Page", "Administration", new {pageId = "new"})'" />

